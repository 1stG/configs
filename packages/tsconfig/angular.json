{
  "$schema": "http://json.schemastore.org/tsconfig",
  "extends": "./ng-lib.json",
  "compilerOptions": {
    "declaration": false,
    "paths": {
      "*": ["src/*"]
    },
    "outDir": "dist",
    "strictFunctionTypes": false,
    "strictNullChecks": false
  },
  "angularCompilerOptions": {
    "enableIvy": true,
    "strictMetadataEmit": false
  }
}
